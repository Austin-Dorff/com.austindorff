<div class="container">
  <div class="welcome">
    <h2 style="text-align:center">Contact Me</h2>
    <br>
    <form class="form-horizontal col-md-9 col-md-offset-3 text-center" action="http://formspree.io/acdorff.dev@gmail.com" method="POST">
      <div class="form-group">
        <label for="_replyto" class="col-sm-2 control-label">Your Email</label>
        <div class="col-sm-4">
          <input class="form-control" type="email" name="_replyto">
        </div>
      </div>
      <div class="form-group">
        <label for="body" class="col-sm-2 control-label">Your Message</label>
        <div class="col-sm-4">
          <textarea class="form-control" rows="4" name="body"></textarea>
        </div>
      </div>
      <div class="form-group">
        <div class="col-sm-4 col-sm-offset-2">
          <input id="submit" name="submit" type="submit" value="Send" class="btn btn-primary">
        </div>
      </div>
      <input type="hidden" name="_next" value="http://austindorff.com/thankyou" />
    </form>
  </div>
</div>
